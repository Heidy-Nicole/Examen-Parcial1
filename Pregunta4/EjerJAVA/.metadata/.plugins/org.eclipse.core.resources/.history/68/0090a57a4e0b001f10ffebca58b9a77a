	<%
		try{
			String apiData = (String) request.getAttribute("apiData");
			
			
			
			for(int i = 0; i <= apiData.length(); i++){
				char x = apiData.charAt(i);
				
				if(x == '[' || x == '{' || x == ']'){
					
				}else{
					if(x == '}' || x == ',')
						out.println('\n');
					else
						out.print(x);
				}
				
			}
			out.println("Datos de la API: " + apiData );
			
		}
		catch(Exception e){
			out.println("Esperando...");
		}
		
	%>
	
